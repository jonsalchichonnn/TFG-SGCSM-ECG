<mapper namespace="com.sgcsm_ecg.mapper.DepartmentMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.sgcsm_ecg.entity.Department">
        <id column="id" property="id" />
